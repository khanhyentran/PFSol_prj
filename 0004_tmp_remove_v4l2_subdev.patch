From 3d2fa414d7aed04ecc7126e7bd9edfc444b56ca8 Mon Sep 17 00:00:00 2001
From: root <khanh.tran.te@renesas.com>
Date: Thu, 9 Aug 2018 09:47:05 +0700
Subject: [PATCH] [tmp]Remove v4l2 subdev call

---
 drivers/media/platform/rza-vin/rza-csi2.c | 8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/drivers/media/platform/rza-vin/rza-csi2.c b/drivers/media/platform/rza-vin/rza-csi2.c
index 1dc6050..16d1ab6 100755
--- a/drivers/media/platform/rza-vin/rza-csi2.c
+++ b/drivers/media/platform/rza-vin/rza-csi2.c
@@ -281,11 +281,11 @@ static int rcar_csi2_start(struct rcar_csi2 *priv, struct v4l2_subdev *nextsd)
 
 	/* Code is validated in set_ftm */
 	format = rcar_csi2_code_to_fmt(priv->mf.code);
-
+/*
 	ret = v4l2_subdev_call(nextsd, video, g_std, &std);
 	if (ret < 0 && ret != -ENOIOCTLCMD && ret != -ENODEV)
 		return ret;
-
+*/
 	if (priv->mf.field != V4L2_FIELD_NONE) {
 		if (std & V4L2_STD_525_60)
 			fld = FLD_FLD_NUM(2);
@@ -381,7 +381,7 @@ static int rcar_csi2_s_stream(struct v4l2_subdev *sd, int enable)
 
 	mutex_lock(&priv->lock);
 
-	ret = rcar_csi2_sd_info(priv, &nextsd);
+	ret = 0;//rcar_csi2_sd_info(priv, &nextsd);
 	if (ret)
 		goto out;
 
@@ -394,7 +394,7 @@ static int rcar_csi2_s_stream(struct v4l2_subdev *sd, int enable)
 			goto out;
 		}
 
-		ret = v4l2_subdev_call(nextsd, video, s_stream, 1);
+		ret = 0;//v4l2_subdev_call(nextsd, video, s_stream, 1);
 		if (ret) {
 			rcar_csi2_stop(priv);
 			pm_runtime_put(priv->dev);
-- 
