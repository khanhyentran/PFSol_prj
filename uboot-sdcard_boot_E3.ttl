setsync 1

ENV_ETHADDR="2e:09:0a:00:83:e1"
ENV_IPADDR="192.168.10.10"
ENV_SERVERIP="192.168.10.30"
ENV_NFSROOT=""
;E3
IMAGE_DTB="Image-r8a77990-ebisu-4d.dtb"
;;##########

sendln "setenv ethaddr " ENV_ETHADDR
waitregex "=> "
pause 1

sendln "setenv serverip " ENV_SERVERIP
waitregex "=> "
pause 1

sendln "setenv ipaddr " ENV_IPADDR
waitregex "=> "
pause 1

sprintf2 s "setenv bootcmd 'fatload mmc 0:1 0x48080000 Image; fatload mmc 0:1 0x48000000 %s; booti 0x48080000 - 0x48000000'" IMAGE_DTB
sendln s
waitregex "=> "
pause 1

sendln "setenv bootargs 'rw root=/dev/mmcblk1p2 rw rootwait mmc_core.removabrootwait mmc_core.removable=0 ip=" ENV_IPADDR "'"
waitregex "=> "
pause 1

sendln "saveenv"
